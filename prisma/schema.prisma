generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Guest {
  id        String  @id @default(uuid()) @map("_id")
  lastName  String
  firstName String?
  address   String
  zip       Int
  city      String
  phone     Int?
}
